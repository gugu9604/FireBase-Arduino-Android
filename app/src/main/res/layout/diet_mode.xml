<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DietMode">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:id="@+id/dietview15"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginTop="645dp"
                android:alpha="0.3"
                android:background="#000000"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/dietroutine1"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="700dp"
                android:layout_marginEnd="80dp"
                android:background="@drawable/imageshape"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/diet_mode_icon" />

            <ImageButton
                android:id="@+id/dietroutine2"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginTop="700dp"
                android:layout_marginEnd="60dp"
                android:background="@drawable/imageshape"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.584"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/diet_mode_icon1" />

            <TextView
                android:id="@+id/diettextView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="660dp"
                android:text="추천 루틴"
                android:textColor="#636161"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/dietview14"
                android:layout_width="200dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="580dp"
                android:layout_marginEnd="60dp"
                android:alpha="0.3"
                android:background="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/dietbluetooth"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="520dp"
                android:background="@drawable/imageshape"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.117"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/dumbel" />

            <TextView
                android:id="@+id/dietView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="540dp"
                android:layout_marginEnd="160dp"
                android:text="블루투스운동"
                android:textColor="#636161"
                android:textSize="18dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/dietview13"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginTop="270dp"
                android:alpha="0.3"
                android:background="#000000"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/dietview12"
                android:layout_width="200dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="200dp"
                android:layout_marginEnd="60dp"
                android:alpha="0.3"
                android:background="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/dietyoutube"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="135dp"
                android:background="@drawable/imageshape"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/youtubeicon" />

            <TextView
                android:id="@+id/diettextView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="160dp"
                android:layout_marginEnd="160dp"
                android:text="운동법&amp; 영상"
                android:textColor="#636161"
                android:textSize="18dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageButton
                android:id="@+id/dietset"
                android:layout_width="370dp"
                android:layout_height="100dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="350dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/imageshape"
                android:backgroundTint="#868383"
                android:scaleType="centerCrop"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/diet_mode_img1" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <View
        android:id="@+id/dietview5"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="90dp"
        android:alpha="0.3"
        android:background="#000000"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/diettoolbar"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_marginTop="35dp"
        android:background="#ffff"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>